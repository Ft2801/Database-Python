# Configurazione della sicurezza per PyInstaller
# Copia questo file in .env e personalizza i valori
# NON committare il file .env effettivo al repository

# Percorso database (opzionale - usa cartella AppData se vuoto)
DATABASE_PATH=

# Percorso file autenticazione (opzionale)
AUTH_PATH=

# Percorso chiave crittografia (opzionale)
KEY_PATH=

# Log level per output build
LOG_LEVEL=INFO

# Modalit√† strict per controlli di sicurezza
STRICT_MODE=true
